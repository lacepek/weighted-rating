{
		"name": "weighted-rating",
		"version": "1.0.0",
		"description": "Vanila JS plugin for weighted rating",
		"main": "index.ts",
		"scripts": {
				"start": "webpack --display-error-details -w --config webpack.dev.js",
				"start:server": "webpack-dev-server --display-error-details --hot --config webpack.dev.js",
				"build:dev": "webpack --display-error-details --config webpack.dev.js",
				"build:prod": "webpack --config webpack.prod.js",
				"build": "npm run build:dev && npm run build:prod",
				"update": "npm i && npm update"
		},
		"dependencies": {
				"nouislider": "^12.1.0"
		},
		"devDependencies": {
				"@types/nouislider": "^9.0.5",
				"clean-css-loader": "^1.1.0",
				"css-loader": "^2.1.0",
				"cssnano": "^4.1.8",
				"optimize-css-assets-webpack-plugin": "^5.0.1",
				"optimize-js-plugin": "0.0.4",
				"style-loader": "^0.23.1",
				"ts-loader": "^4.5.0",
				"typescript": "^3.2.2",
				"uglifyjs-webpack-plugin": "^1.3.0",
				"webpack": "^4.28.3",
				"webpack-cli": "^3.2.1",
				"webpack-dev-server": "^3.1.14"
		},
		"keywords": [
				"js",
				"plugin"
		],
